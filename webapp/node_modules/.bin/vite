#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/vite@5.4.0_@types+node@14.18.63_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/bin/node_modules:/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/vite@5.4.0_@types+node@14.18.63_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/node_modules:/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/vite@5.4.0_@types+node@14.18.63_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules:/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/vite@5.4.0_@types+node@14.18.63_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/bin/node_modules:/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/vite@5.4.0_@types+node@14.18.63_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules/vite/node_modules:/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/vite@5.4.0_@types+node@14.18.63_less@4.2.0_sass@1.77.6_terser@5.31.6/node_modules:/opt/shared/palmyra/adridict/2214057/frontapplicationdemo-frontend/webapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
